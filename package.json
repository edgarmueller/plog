{
  "name": "plog-frontend",
  "version": "0.1.0",
  "description": "Frontend for plog",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env NODE_ENV=test jest",
    "eject": "react-scripts eject",
    "test-only": "cross-env NODE_ENV=test jest --no-cache test/components/Editor.spec.js",
    "test-with-coverage": "cross-env NODE_ENV=test jest --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/registerServiceWorker.js",
    "test-with-coveralls": "cross-env NODE_ENV=test jest --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/registerServiceWorker.js && cat ./coverage/lcov.info | coveralls",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rimraf build",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edgarmueller/plog-frontend"
  },
  "author": "Edgar Mueller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edgarmueller/plog-frontend/issues"
  },
  "homepage": "http://plog-frontend.herokuapp.com/",
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "./node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!src/registerServiceWorker.js",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/test/**",
      "!**/public/**",
      "!**/vendor/**",
      "!**/coverage/**",
      "!**/__mocks__/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "html"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "exclude": [
      "src/index.js",
      "src/components/configureStore.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "ava": "0.25.0",
    "axios-mock-adapter": "^1.10.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.5.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "expect": "^23.0.0-alpha.0",
    "file-api": "^0.10.4",
    "file-loader": "^1.1.11",
    "filereader": "^0.10.3",
    "fixed-data-table": "^0.6.3",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "jest": "^22.0.3",
    "jsdom": "^11.5.1",
    "json-server": "^0.12.1",
    "nyc": "11.6.0",
    "postcss-loader": "^2.1.2",
    "postcss-nested": "^3.0.0",
    "react-fake-props": "^0.2.1",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "sinon": "^4.4.6",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1"
  },
  "dependencies": {
    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.18.0",
    "brace": "^0.11.1",
    "classnames": "^2.2.5",
    "dotenv": "^6.1.0",
    "dropbox": "^4.0.9",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.11.1",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "jwt-decode": "^2.1.0",
    "katex": "^0.9.0-alpha2",
    "lodash": "^4.15.0",
    "markdown-it": "^8.4.0",
    "markdown-it-katex": "^2.0.3",
    "material-ui": "^1.0.0-beta.36",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.20.1",
    "mousetrap": "^1.6.1",
    "org": "^0.2.0",
    "prop-types": "^15.5.8",
    "radium": "^0.23.0",
    "react": "^16.5.2",
    "react-ace": "^6.2.0",
    "react-autosuggest": "^9.3.2",
    "react-dates": "^16.0.1",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.1.2",
    "react-file-download": "^0.3.4",
    "react-file-input": "^0.2.5",
    "react-hot-loader": "^4.0.0",
    "react-markdown": "^3.3.0",
    "react-oauth-flow": "^1.2.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-router-role-authorization": "^1.1.3",
    "react-scripts": "^1.0.17",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^2.0.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.1.0",
    "roboto-fontface": "^0.9.0",
    "typeface-roboto": "0.0.54"
  }
}
