{
  "name": "plog-frontend",
  "version": "0.1.0",
  "description": "Frontend for plog",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "webpack-dev-server",
    "build": "webpack -p --progress --config webpack.production.config.js",
    "test-only": "ava test/config/Routes.spec.js",
    "test": "ava",
    "test-with-coverage": "set BABEL_DISABLE_CACHE=1&&set NODE_ENV=AVA&&nyc ava",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm build/app.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edgarmueller/plog-frontend"
  },
  "author": "Edgar Mueller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edgarmueller/plog-frontend/issues"
  },
  "homepage": "https://github.com/edgarmueller/plog-frontend",
  "ava": {
    "verbose": true,
    "require": [
      "babel-polyfill",
      "babel-register",
      "ignore-styles"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "exclude": [
      "src/index.js",
      "src/components/configureStore.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "autoprefixer": "^6.0.2",
    "ava": "^0.18.2",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.5.2",
    "coveralls": "^2.11.2",
    "css-loader": "^0.26.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "fixed-data-table": "^0.6.3",
    "ignore-styles": "^5.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^9.8.3",
    "jsdom-global": "2.1.1",
    "json-server": "^0.9.4",
    "nyc": "^10.1.2",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.5.4",
    "redux-logger": "^2.7.0",
    "redux-mock-store": "^1.2.1",
    "sass-loader": "^4.0.1",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.5.2",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "brace": "^0.10.0",
    "classnames": "^2.2.5",
    "data.maybe": "^1.2.2",
    "form-data": "^2.1.2",
    "immutable": "^3.8.1",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.15.0",
    "material-ui": "^0.18.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-ace": "^5.1.0",
    "react-dom": "^15.5.4",
    "react-dropzone": "^3.7.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-latex": "^1.0.1",
    "react-markdown": "^2.5.0",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-router-role-authorization": "^1.1.3",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^0.10.0",
    "redux-form": "^6.8.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "reflexbox": "^3.0.0-0",
    "roboto-fontface": "^0.7.0",
    "typeface-roboto": "0.0.22"
  }
}
