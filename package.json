{
  "name": "plog-frontend",
  "version": "0.1.0",
  "description": "Frontend for plog",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env NODE_ENV=test ava",
    "eject": "react-scripts eject",
    "test-only": "cross-env NODE_ENV=test ava test/components/Tag.spec.js",
    "test-with-coverage": "cross-env NODE_ENV=test nyc ava",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rimraf build",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edgarmueller/plog-frontend"
  },
  "author": "Edgar Mueller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edgarmueller/plog-frontend/issues"
  },
  "homepage": "http://plog-frontend.herokuapp.com/",
  "ava": {
    "verbose": true,
    "require": [
      "babel-polyfill",
      "babel-register",
      "ignore-styles"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "exclude": [
      "src/index.js",
      "src/components/configureStore.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "autoprefixer": "^6.0.2",
    "ava": "0.22.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.5.2",
    "coveralls": "^2.11.2",
    "cross-env": "^5.1.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.8.2",
    "expect": "^1.20.2",
    "file-api": "^0.10.4",
    "file-loader": "^0.9.0",
    "filereader": "^0.10.3",
    "fixed-data-table": "^0.6.3",
    "ignore-styles": "^5.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "9.11.0",
    "json-server": "^0.9.4",
    "nyc": "10.0.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-fake-props": "^0.1.3",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.1",
    "sass-loader": "^4.0.1",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  },
  "dependencies": {
    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.15.3",
    "brace": "^0.10.0",
    "classnames": "^2.2.5",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "immutable": "^3.8.1",
    "jwt-decode": "^2.1.0",
    "katex": "^0.9.0-alpha2",
    "lodash": "^4.15.0",
    "markdown-it": "^8.4.0",
    "markdown-it-katex": "^2.0.3",
    "material-ui": "^1.0.0-beta.22",
    "material-ui-icons": "^1.0.0-beta.17",
    "mousetrap": "^1.6.1",
    "npm": "^5.5.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-ace": "^5.1.0",
    "react-autosuggest": "^9.3.2",
    "react-dom": "^15.5.4",
    "react-dropzone": "^4.1.2",
    "react-file-download": "^0.3.4",
    "react-file-input": "^0.2.5",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-markdown": "^2.5.0",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-router-role-authorization": "^1.1.3",
    "react-scripts": "^1.0.17",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^2.0.2",
    "redux-form": "^6.8.0",
    "redux-form-material-ui": "^5.0.0-beta.2",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.6.2",
    "roboto-fontface": "^0.7.0",
    "typeface-roboto": "0.0.22"
  }
}
